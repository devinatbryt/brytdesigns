{
  "name": "@brytdesigns/shopify-hybrid-cart-api",
  "version": "1.1.0",
  "description": "A hybrid cart that integrates with the AJAX Cart API and the Storefront Api",
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen",
    "test": "vitest"
  },
  "main": "./dist/main/index.js",
  "types": "./dist/main/index.d.ts",
  "jsdelivr": "./dist/jsdelivr/index.js",
  "module": "./dist/main/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./effect": {
      "types": "./dist/effect.d.ts",
      "import": "./dist/effect.js"
    }
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@effect/vitest": "^0.19.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@solidifront/codegen": "^1.0.7",
    "dts-bundle-generator": "^9.5.1",
    "tsup": "^8.4.0",
    "vitest": "^3.0.6",
    "@repo/js-delivr-imports-plugin": "workspace:*"
  },
  "dependencies": {
    "@brytdesigns/shopify-utils": "workspace:*",
    "@effect/platform": "^0.77.4",
    "@solidifront/storefront-client": "^0.3.3",
    "effect": "3.13.4"
  }
}
